CREATE TABLE IF NOT EXISTS `news` (
`id` INT  PRIMARY KEY AUTO_INCREMENT,
`title` VARCHAR(1000) DEFAULT '',
`img` INT,
`shorthand` TEXT DEFAULT '',
`content`  MEDIUMTEXT,
`published` DATETIME,
`views`  INT,
`user_type` INT,
`data` text  DEFAULT ''
)---
